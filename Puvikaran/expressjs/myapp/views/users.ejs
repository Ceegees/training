<!DOCTYPE html>
<html>
  <head>
<% include partials/head %>
  </head>
  <body>
  <header>
    <% include partials/header %>
  </header>
   
  <main>
    <table class="table table-bordered">
      <thead>
        <tr>
          <th>Id</th>
          <th>Name</th>
          <th>Email</th>
          <th>Location</th>
        </tr>
      </thead>
      <tbody>
      <% for(var i=0; i< results.length; i++) { %>
        <tr>      
          <td><%= results[i].id %></td>
          <td><%= results[i].name %></td>
          <td><%= results[i].email %></td>
          <td><%= results[i].location %></td>
        </tr>
      <% } %>
      </tbody>
    </table>

  </main>
  <footer>
    <% include partials/footer %>
  </footer>

  </body>
</html>
